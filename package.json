{
  "name": "gdax-client",
  "version": "0.0.2",
  "description": "",
  "scripts": {
    "dev": "concurrently 'babel-node ./server/server.js' 'next' ",
    "precommit": "lint-staged",
    "server": "babel-node ./server/server.js",
    "test": "lint-staged && eslint ."
  },
  "author": {
    "name": "Andy Chen",
    "email": "amazingandyyy@gmail.com",
    "url": "https://amazingandyyy.github.io/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amazingandyyy/gdax-client"
  },
  "license": "MIT",
  "dependencies": {
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "fs": "^0.0.1-security",
    "gdax": "^0.4.2",
    "next": "^2.4.4",
    "next-redux-wrapper": "^1.1.2",
    "path": "^0.12.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^7.0.31",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-watch": "^2.0.6",
    "concurrently": "^3.4.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "lint-staged": "^3.4.2",
    "tslint-eslint-rules": "^4.1.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "babel-preset-env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react/react-in-jsx-scope": 0,
      "no-console": 0,
      "react/display-name": 0,
      "react/prop-types": 0
    },
    "globals": {
      "React": true
    },
    "env": {
      "node": true,
      "es6": true
    }
  }
}
